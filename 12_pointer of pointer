// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int i=3;
    int *ptr=&i;
    int **ptr_ptr=&ptr;
    printf("The value of i is %d\n",*ptr);
    printf("The value of i is %d\n",**ptr_ptr);
    printf("The location of i is %d\n",ptr);
    printf("The location of ptr is %d\n",ptr_ptr);

    return 0;
}
