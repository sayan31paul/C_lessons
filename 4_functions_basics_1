// Online C compiler to run C program online
#include <stdio.h>
int sum(int a,int b);//sum is a function which takes a and b as input and returns an integer as an output. This is function prototype declaration.

    
    

int main() {
    int c;
    c=sum(2,5); //function call.
    printf("%d",c);

    return 0;
}

int sum(int a,int b){
    int result;
    result=a+b;
    return result;
}
